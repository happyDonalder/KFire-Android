<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<SolutionRoot Condition="'$(SolutionRoot)' == ''">$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), NuGetApi.sln)</SolutionRoot>
	</PropertyGroup>
	<ItemGroup>
	    <Content Include="$(SolutionRoot)\src\Common\AzureStartupScripts\EnableServerGC.ps1">
	      <Link>Startup\EnableServerGC.ps1</Link>
	      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	    </Content>
	    <Content Include="$(SolutionRoot)\src\Common\AzureStartupScripts\posh.cmd">
	      <Link>Startup\posh.cmd</Link>
	      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	    </Content>
	</ItemGroup>
</Project>